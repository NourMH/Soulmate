@model project74.Models.image

@{
    ViewBag.Title = "Index";
}

<div class="container"style="margin-top:7em;" >
    <div class="col-md-7 col-sm-11">
        <form class="form" method="post" enctype="multipart/form-data">
            <h2 class=" title form-group">Upload Images</h2>
            <div class="form-group">
                <label >Package Name</label>
                @Html.DropDownListFor(n => n.p_id, (SelectList)ViewBag.package, new { @style = "color:black;width:99%;height:3em;border: 1px solid #C4698A;", @class = "form-group  m-1" })
            </div>

            <div class="form-group">
                <label>Images</label>
                <input class="form-control-file w-75 m-1" name="fileobj" type="file" style="color:black;" multiple>
                
            </div>
            <div class="form-group">
                <input type="submit" value="upload images" class="btn btn-danger text-white m-1" style="background-color: #C4698A" />
                <br>
                @Html.ActionLink("Back to List", "Imgindex",null, new { @style = "display:block;margin-top: 1em;" })
            </div>
            <hr>
            <h4>@ViewData["Message"]</h4>
        </form>
    </div>
</div>
